<?php

namespace Acme\StoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Hosotthc
 *
 * @ORM\Table(name="hosotthc")
 * @ORM\Entity
 */
class Hosotthc
{
    /**
     * @var string
     *
     * @ORM\Column(name="idTTHC", type="string", length=45, nullable=false)
     */
    private $idtthc;

    /**
     * @var integer
     *
     * @ORM\Column(name="idCongDan", type="integer", nullable=false)
     */
    private $idcongdan;

    /**
     * @var integer
     *
     * @ORM\Column(name="idDonViThuLy", type="integer", nullable=false)
     */
    private $iddonvithuly;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="NgayNhan", type="date", nullable=false)
     */
    private $ngaynhan;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="NgayHoiDap", type="date", nullable=false)
     */
    private $ngayhoidap;

    /**
     * @var string
     *
     * @ORM\Column(name="KetQuaThuLy", type="string", length=255, nullable=false)
     */
    private $ketquathuly;

    /**
     * @var string
     *
     * @ORM\Column(name="DinhKemGui", type="string", length=45, nullable=true)
     */
    private $dinhkemgui;

    /**
     * @var string
     *
     * @ORM\Column(name="DinhKemNhan", type="string", length=45, nullable=true)
     */
    private $dinhkemnhan;

    /**
     * @var string
     *
     * @ORM\Column(name="TinhTrangHoSo", type="string", length=45, nullable=true)
     */
    private $tinhtranghoso;

    /**
     * @var integer
     *
     * @ORM\Column(name="idHoSoTTHC", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idhosotthc;


}
