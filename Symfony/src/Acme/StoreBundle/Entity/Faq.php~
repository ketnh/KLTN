<?php

namespace Acme\StoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Faq
 *
 * @ORM\Table(name="faq")
 * @ORM\Entity
 */
class Faq
{
    /**
     * @var integer
     *
     * @ORM\Column(name="idDichVu", type="integer", nullable=false)
     */
    private $iddichvu;

    /**
     * @var string
     *
     * @ORM\Column(name="NDCauHoi", type="string", length=45, nullable=false)
     */
    private $ndcauhoi;

    /**
     * @var string
     *
     * @ORM\Column(name="NDCauTraLoi", type="string", length=45, nullable=true)
     */
    private $ndcautraloi;

    /**
     * @var integer
     *
     * @ORM\Column(name="idFAQ", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idfaq;


}
