<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\KLBundle\Entity\Congdan" table="congdan">
    <id name="idcongdan" type="integer" column="idCongDan">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="Name" length="45" nullable="false"/>
    <field name="ngaysinh" type="date" column="Ngaysinh" nullable="false"/>
    <field name="quequan" type="string" column="QueQuan" length="45" nullable="false"/>
    <field name="sdt" type="string" column="SDT" length="45" nullable="false"/>
    <field name="email" type="string" column="email" length="45" nullable="false"/>
  </entity>
</doctrine-mapping>
